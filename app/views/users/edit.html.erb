<%= form_with model: @user do |form| %>
    <%= form.label :Username%>
    <%= form.text_field :username , placeholder: 'username'%>
    <br>
    <%= form.label :email %>
    <%= form.text_field :email ,placeholder: 'example@email.com'%>
    <br>
    <%= form.label :password %>
    <%= form.password_field :password %>
    <%= form.submit "Create" %>
<%end%>